<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>meizitu</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>

	<!-- 查询girl列表 -->
	<servlet>
		<servlet-name>groupImageInfoList</servlet-name>
		<servlet-class>com.kubeiwu.servlet.GroupImageInfoListServlet</servlet-class>
 
	</servlet>
	<servlet-mapping>
		<servlet-name>groupImageInfoList</servlet-name>
		<url-pattern>/groupImageInfoList</url-pattern>
	</servlet-mapping>
	<!-- 查询girl详情 -->
	<servlet>
		<servlet-name>groupImageInfoDetails</servlet-name>
		<servlet-class>com.kubeiwu.servlet.GroupImageInfoDetailsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>groupImageInfoDetails</servlet-name>
		<url-pattern>/groupImageInfoDetails</url-pattern>
	</servlet-mapping>
	<!-- 登陆 -->
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>com.kubeiwu.servlet.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>

	<!-- 添加数据 -->
	<servlet>
		<servlet-name>insertGroupImageInfo</servlet-name>
		<servlet-class>com.kubeiwu.servlet.InsertGroupImageInfoServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>insertGroupImageInfo</servlet-name>
		<url-pattern>/insertGroupImageInfo</url-pattern>
	</servlet-mapping>
	<!-- 类型列表 -->
	<servlet>
		<servlet-name>categoryList</servlet-name>
		<servlet-class>com.kubeiwu.servlet.CategoryListServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>categoryList</servlet-name>
		<url-pattern>/categoryList</url-pattern>
	</servlet-mapping>
	<!-- 大型的类型列表 -->
	<servlet>
		<servlet-name>sourcecategoryList</servlet-name>
		<servlet-class>com.kubeiwu.servlet.SourceCategoryListServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>sourcecategoryList</servlet-name>
		<url-pattern>/sourcecategoryList</url-pattern>
	</servlet-mapping>
</web-app>