<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kubeiwu.dao.IGirlImage">
	<resultMap type="com.kubeiwu.bean.GirlImage" id="GirlImageResult">
		<id column="GID" jdbcType="INTEGER" property="id" />
		<result column="IMAGEURL" jdbcType="VARCHAR" property="imageurl" />
		<result column="GIRLINFO_ID" jdbcType="INTEGER" property="girlinfo_id" />
	</resultMap>
<!-- insert 插入 replace 替换插入 -->
	<insert id="insertOne" parameterType="com.kubeiwu.bean.GirlImage">
		insert ignore into GIRLIMAGE(IMAGEURL,GIRLINFO_ID)
		value(#{imageurl},#{girlinfo_id});
	</insert>
	<insert id="insertBatch" parameterType="java.util.List" statementType="PREPARED" keyProperty="id">
		insert ignore into GIRLIMAGE(IMAGEURL,GIRLINFO_ID)
		values
		<foreach collection="list" item="item" separator=",">
			(#{item.imageurl},#{item.girlinfo_id})
		</foreach>

	</insert>
</mapper>